{
  "name": "touhou-local-scoreboards",
  "displayName": "Touhou Local Scoreboards",
  "version": "0.5.0",
  "author": "imo-tikuwa",
  "description": "東方Projectの原作シューティングゲームのリプレイデータを管理することができるツールです",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "electron",
    "vue",
    "vite"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "fmt": "prettier --write '**/*.{vue,ts,json,css,scss,md}'",
    "compile": "npm run build && electron-builder --config electron-builder.local.yml"
  },
  "devDependencies": {
    "@primevue/auto-import-resolver": "^4.1.1",
    "@primevue/themes": "^4.1.1",
    "@types/file-saver": "^2.0.7",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vueuse/core": "^11.1.0",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "fflate": "^0.8.2",
    "file-saver": "^2.0.5",
    "gulp": "^5.0.0",
    "gulp-zip": "^6.0.0",
    "idb": "^8.0.0",
    "prettier": "^3.3.3",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.1.1",
    "thrpy-parser": "^0.2.1",
    "typescript": "^5.6.3",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.9",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5",
    "vue-tsc": "^2.1.6"
  }
}
